<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ImageWithText">
        <attr name="view_image" format="reference"/>
        <attr name="view_text" format="reference|string"/>
        <attr name="view_textColor" format="color"/>
        <attr name="view_textSize" format="dimension"/>
        <attr name="view_focusable" format="boolean"/>
        <!--<attr name="background" format="reference|color"/>-->
    </declare-styleable>
    <declare-styleable name="CustomImageView">
        <attr name="civ_type" format="enum">
            <enum name="TYPE_DEFULTE" value="0"/>
            <enum name="TYPE_CIRCLE" value="1"/>
            <enum name="TYPE_ROUND" value="2"/>
            <enum name="TYPE_OVAL" value="3"/>
        </attr>
        <attr name="civ_roundradius" format="integer"/>
    </declare-styleable>
</resources>